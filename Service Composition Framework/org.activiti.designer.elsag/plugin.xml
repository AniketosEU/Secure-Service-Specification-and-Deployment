<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.activiti.designer.elsag.application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.activiti.designer.eclipse.common.Application">
         </run>
      </application>
   </extension>
   <extension
         id="ServiceCompositionFramework"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.activiti.designer.elsag.application"
            name="Service Composition Framework">
         <property
               name="windowImages"
               value="icons/aniketos.png,icons/aniketos.png,icons/aniketos.png,icons/aniketos.png,icons/aniketos.png">
         </property>
         <property
               name="aboutText"
               value="Version 1.4.5">
         </property>
         <property
               name="aboutImage"
               value="icons/aniketos.png">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="20,200,410,15">
         </property>
         <property
               name="startupMessageRect"
               value="22,180,410,20">
         </property>
         <property
               name="appName"
               value="Service Composition Framework">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.activiti.designer.eclipse.perspective.SCFPerspective"
            fixed="true"
            icon="icons/aniketos.png"
            id="org.activiti.designer.eclipse.perspective.SCFPerspective"
            name="Service Composition Framework">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="settings"
               label="Settings">
            <command
                  commandId="org.activiti.designer.elsag.settingsDialog"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.window.preferences"
                  label="General"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.activiti.designer.elsag.handler.SettingsHandler"
            id="org.activiti.designer.elsag.settingsDialog"
            name="Network">
      </command>
   </extension>
   <extension
         id="ServiceCompositionFrameworkSAP"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.activiti.designer.elsag.application"
            name="Service Composition Framework">
         <property
               name="windowImages"
               value="icons/aniketos.png,icons/aniketos.png,icons/aniketos.png,icons/aniketos.png,icons/aniketos.png">
         </property>
         <property
               name="aboutText"
               value="Service Composition Framework">
         </property>
         <property
               name="aboutImage"
               value="icons/aniketos.png">
         </property>
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="20,200,410,15">
         </property>
         <property
               name="startupMessageRect"
               value="22,180,410,20">
         </property>
      </product>
   </extension>
