        <hiddenToolBarItem
              id="org.eclipse.ui.edit.text.gotoLastEditPosition">
        </hiddenToolBarItem>
        <hiddenToolBarItem
              id="org.eclipse.debug.ui.launchActionSet">
        </hiddenToolBarItem>
        <hiddenMenuItem
              id="org.eclipse.jdt.ui.refactoring.menu">
        </hiddenMenuItem>
        <hiddenMenuItem
              id="org.eclipse.jdt.ui.source.menu">
        </hiddenMenuItem>
        <hiddenMenuItem
              id="org.eclipse.ui.edit.text.openLocalFile">
        </hiddenMenuItem>
        <hiddenMenuItem
              id="org.eclipse.ui.edit.text.delimiter.windows">
        </hiddenMenuItem>
        <hiddenMenuItem
              id="org.eclipse.ui.edit.text.delimiter.macOS9">
        </hiddenMenuItem>
        <hiddenMenuItem
              id="org.eclipse.ui.edit.text.delimiter.unix">
        </hiddenMenuItem>
        <hiddenMenuItem
              id="org.eclipse.ui.window.showKeyAssist">
        </hiddenMenuItem>
     </perspectiveExtension>
  </extension>
